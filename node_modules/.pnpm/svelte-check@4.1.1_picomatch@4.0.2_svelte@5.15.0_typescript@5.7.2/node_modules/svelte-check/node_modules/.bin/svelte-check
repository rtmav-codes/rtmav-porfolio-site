#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/rtmav/rtmav-porfolio-site/my-app/node_modules/.pnpm/svelte-check@4.1.1_picomatch@4.0.2_svelte@5.15.0_typescript@5.7.2/node_modules/svelte-check/bin/node_modules:/Users/rtmav/rtmav-porfolio-site/my-app/node_modules/.pnpm/svelte-check@4.1.1_picomatch@4.0.2_svelte@5.15.0_typescript@5.7.2/node_modules/svelte-check/node_modules:/Users/rtmav/rtmav-porfolio-site/my-app/node_modules/.pnpm/svelte-check@4.1.1_picomatch@4.0.2_svelte@5.15.0_typescript@5.7.2/node_modules:/Users/rtmav/rtmav-porfolio-site/my-app/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/rtmav/rtmav-porfolio-site/my-app/node_modules/.pnpm/svelte-check@4.1.1_picomatch@4.0.2_svelte@5.15.0_typescript@5.7.2/node_modules/svelte-check/bin/node_modules:/Users/rtmav/rtmav-porfolio-site/my-app/node_modules/.pnpm/svelte-check@4.1.1_picomatch@4.0.2_svelte@5.15.0_typescript@5.7.2/node_modules/svelte-check/node_modules:/Users/rtmav/rtmav-porfolio-site/my-app/node_modules/.pnpm/svelte-check@4.1.1_picomatch@4.0.2_svelte@5.15.0_typescript@5.7.2/node_modules:/Users/rtmav/rtmav-porfolio-site/my-app/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/svelte-check" "$@"
else
  exec node  "$basedir/../../bin/svelte-check" "$@"
fi
